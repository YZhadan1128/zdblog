<%- include('../layout')%>
<div class="container-fluid">
  <ol class="breadcrumb">
    <li><a href="javascript:;">分类首页</a></li>
    <li class="active">分类删除</li>
  </ol>
  <h3>分类删除 - <%= category %></h3>

  <form method="post">
